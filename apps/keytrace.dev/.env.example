# Public URL (your domain)
NUXT_PUBLIC_PUBLIC_URL=https://keytrace.dev

# Session secret (generate a secure random string for production)
NUXT_SESSION_SECRET=your-secret-key-here

# S3 configuration for session storage
NUXT_S3_BUCKET=keytrace-sessions
NUXT_S3_REGION=fr-par
NUXT_S3_ACCESS_KEY_ID=your-access-key
NUXT_S3_SECRET_ACCESS_KEY=your-secret-key

# Optional: custom endpoint (defaults to https://s3.{region}.scw.cloud)
# NUXT_S3_ENDPOINT=https://s3.fr-par.scw.cloud

# Keytrace service account credentials (for writing claims to user repos)
NUXT_KEYTRACE_DID=did:plc:your-service-account-did
NUXT_KEYTRACE_PASSWORD=your-app-password
